{
  "name": "zendare-app",
  "private": true,
  "version": "0.0.1",
  "description": "zendare-app",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "clean-build": "rm -rf ./www && yarn build && yarn build-sw",
    "clean-build-dev": "rm -rf ./www && stencil build --dev && yarn build-sw-dev",
    "start": "stencil build --dev --watch --serve",
    "build-sw": " cd sw && yarn sw-build && cd - && workbox injectManifest workbox-config.js ",
    "update-ui": " cd ui && yarn build && cd - && yarn add ./ui ",
    "build-sw-dev": " cd sw && yarn sw-build-dev && cd - && workbox injectManifest workbox-config.js ",
    "build-release": "yarn update-ui && yarn build && yarn build-sw",
    "test": "npx jest test",
    "test_e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@capacitor/core": "^4.4.0",
    "@capacitor/device": "^4.0.1",
    "@capacitor/network": "^4.0.1",
    "@capacitor/preferences": "^4.0.1",
    "@ionic/core": "^6.3.4",
    "@sentry/browser": "^7.18.0",
    "chalk": "^4.1.2",
    "date-fns": "^2.29.3",
    "deep-object-diff": "^1.1.7",
    "firebase": "^9.13.0",
    "loglevel": "^1.8.0",
    "marked": "^2.1.3",
    "nanoid": "^4.0.0",
    "qs": "^6.10.2",
    "ramda": "^0.28.0",
    "rxfire": "^6.0.3",
    "rxjs": "^7.5.7",
    "superstruct": "^0.15.3",
    "zendare-ui": "./ui"
  },
  "devDependencies": {
    "@stencil/core": "2.18.1",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^2.0.0",
    "@stencil/store": "^1.5.0",
    "@types/jest": "^27.0.3",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.11.9",
    "@types/puppeteer": "5.4.4",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.27.64",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^27.3.1",
    "jest-cli": "27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "puppeteer": "11.0.0",
    "purgecss": "^4.0.3",
    "rollup": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-purgecss": "^4.0.3",
    "rollup-plugin-visualizer": "^5.5.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.8.4"
  }
}
